@page
@model SparkAuto.Pages.Users.DeleteModel
@{
}
<div class="col-md-8">
    <form method="post">
        <input asp-for="ApplicationUser.Id" hidden />

        <div class="border backgroundWhite">

            <div class="row">
                <div class="col-6">
                    <h2 class="text-info">Delete Customer </h2>
                </div>
            </div>
            <br />

            <div class="form-group">
                <div class="row">
                    <div class="col-4">
                        <label asp-for="ApplicationUser.Name"></label>
                    </div>
                    <div class="col-8">
                        <input asp-for="ApplicationUser.Name" disabled class="form-control" />
                       
                    </div>

                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-4">
                        <label asp-for="ApplicationUser.Email"></label>
                    </div>
                    <div class="col-8">

                        <input asp-for="ApplicationUser.Email" disabled class="form-control" />
                    </div>

                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-4">
                        <label asp-for="ApplicationUser.PhoneNumber"></label>
                    </div>
                    <div class="col-8">
                        <input asp-for="ApplicationUser.PhoneNumber" disabled class="form-control" />
               
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-4">
                        <label asp-for="ApplicationUser.Address"></label>
                    </div>
                    <div class="col-8">
                        <input asp-for="ApplicationUser.Address" disabled class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-4">
                        <label asp-for="ApplicationUser.City"></label>
                    </div>
                    <div class="col-8">
                        <input asp-for="ApplicationUser.City" disabled class="form-control" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-4">
                        <label asp-for="ApplicationUser.PostalCode"></label>
                    </div>
                    <div class="col-8">
                        <input asp-for="ApplicationUser.PostalCode" disabled class="form-control" />
                    </div>
                </div>
            </div>

            <div class="form-group row">
                <div class="col-6 offset-4">
                    <div class="row">
                        <div class="col-6">
                            <input type="submit" value="Delete" class="btn btn-danger form-control" />
                        </div>
                        <div class="col-6">
                            <a asp-page="Index" class="btn btn-success form-control">Back to List </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>

</div>
@section Scripts{
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

